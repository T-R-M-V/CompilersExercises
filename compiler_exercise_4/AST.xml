<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ProgramOpNode>
    <VarDeclOpNode>
        <VarOptInitOpNode>
            <IdentifierNode identifier="x"/>
        </VarOptInitOpNode>
        <VarOptInitOpNode>
            <IdentifierNode identifier="y"/>
            <ExprValueNode>
                <ConstantNode type="Integer" value="10"/>
            </ExprValueNode>
        </VarOptInitOpNode>
        <typeOrConstantNode>
            <TypeNode Type="Integer"/>
        </typeOrConstantNode>
    </VarDeclOpNode>
    <beginEndOpNode>
        <ReadOpNode>
            <IdentifierNode identifier="x"/>
            <IdentifierNode identifier="y"/>
        </ReadOpNode>
        <IfThenOpNode>
            <BinaryOpNode type="Gt">
                <ExprValueNode>
                    <IdentifierNode identifier="x"/>
                </ExprValueNode>
                <ExprValueNode>
                    <IdentifierNode identifier="y"/>
                </ExprValueNode>
            </BinaryOpNode>
            <body>
                <WriteOpNode newline="true">
                    <ExprValueNode>
                        <ConstantNode type="String" value="I valori di x e y sono:"/>
                    </ExprValueNode>
                    <ExprValueNode>
                        <IdentifierNode identifier="x"/>
                    </ExprValueNode>
                    <ExprValueNode>
                        <ConstantNode type="String" value=" "/>
                    </ExprValueNode>
                    <ExprValueNode>
                        <IdentifierNode identifier="y"/>
                    </ExprValueNode>
                </WriteOpNode>
            </body>
        </IfThenOpNode>
    </beginEndOpNode>
</ProgramOpNode>
